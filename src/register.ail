CREATE CLASS AXC(user_profile) IN scouts ->[];
CREATE CLASS AXC(user_projects) IN scouts ->[];
CREATE CLASS AXC(user_story) IN scouts ->[];
CREATE CLASS AXC(user_cart) IN scouts ->[];
CREATE CLASS AXC(user_order) IN scouts ->[];
